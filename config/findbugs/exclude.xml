<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!-- http://findbugs.sourceforge.net/manual/filter.html -->
<FindBugsFilter>

  <!-- By Design -->
  <Match>
    <Class name="com.dkorobtsov.logging.AbstractInterceptor"/>
    <Bug code="UWF_UNWRITTEN_PUBLIC_OR_PROTECTED_FIELD"/>
  </Match>

  <!-- By Design -->
  <Match>
    <Class name="com.dkorobtsov.logging.internal.BodyFormatter"/>
    <Bug code="REC_CATCH_EXCEPTION"/>
  </Match>

  <!-- False Positive -->
  <Match>
    <Class name="com.dkorobtsov.logging.interceptors.apache.ApacheEntityUtil"/>
    <Bug code="DM_DEFAULT_ENCODING"/>
  </Match>

</FindBugsFilter>
